extends layout

block content
	#categories-page.content
		.d-flex.justify-content-center
			- for ( var i = 0; i < categories.length; i += 1 )
				- var category = categories[i]
					.category
						a.cell(href="categories/" +category.id)
							.context
								img(src=category.image)
							h2= category.title