.col-lg-12
  .row.card
    .col-lg-2.avatar
    .col-lg-10
      a(href='/members/' + member.id)
        h1.member-name= member.first_name + ' ' + member.last_name
      - var p = 12
      p= 'Working on ' + helper.pluralize('project', member.numberOfProjects, true)
      .quick-links
        each link in member.links
          a(href=link target='_blank').links
            img.links
