extends layout-dashboard

block view
	#projects-page.content
		each project in projects
			- var canCRUD = token.role == 1 || project.admin == true ? true : false;
			a(href= canCRUD ? "projects/" + project.id : "#" class= !canCRUD ? 'disabled' : '').cell
				.context
					p.header= project.title
					p= helper.pluralize('member', project.members.length, true)
					p= project.description_short
